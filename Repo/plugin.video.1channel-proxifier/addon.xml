<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.1channel-proxifier" name="1Channel Proxifier" version="1.0.5" provider-name="eleazarcoding">
  <requires>
    <import addon="xbmc.python" version="2.1" />
    <import addon="plugin.video.1channel" version="2.0.2" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">1Channel Proxifier</summary>
    <description lang="en">This temporary fix will enable immediate use of the PrimewireProxy.co.uk service for 1Channel users blocked due to censorship in the United Kingdom, allowing you to once again use 1Channel freely. Please visit www.xbmchub.com for assistance with this add-on.</description>
    <platform>all</platform>
    <forum>http://www.xbmchub.com/forums/eleazars-xbmc-add-repository/</forum>
    <website>http://www.xbmchub.com/</website>
  </extension>
</addon>